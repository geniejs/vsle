{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "npm-run-all -p build:*",
    "servertsc": "tsc -p tsconfig.server.json",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "cross-env NODE_ENV=production remix build",
    "dev": "npm-run-all -p dev:*",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "cross-env NODE_ENV=development remix watch",
    "generate:css": "npx tailwindcss -i ./styles/tailwind.css -o ./app/tailwind.css",
    "start": "cross-env NODE_ENV=production npm run wrangler",
    "typecheck": "tsc -b",
    "dev:wrangler": "cross-env NODE_ENV=development npm run wrangler",
    "wrangler": "wrangler pages dev ./public",
    "build:worker": "esbuild ./app/entry.worker.ts --outfile=./public/entry.worker.js --minify --bundle --format=esm --define:process.env.NODE_ENV='\"production\"'",
    "dev:worker": "esbuild ./app/entry.worker.ts --outfile=./public/entry.worker.js --bundle --format=esm --define:process.env.NODE_ENV='\"development\"' --watch",
    "postinstall": "remix-esbuild-override && patch-package"
  },
  "dependencies": {
    "@auth/core": "^0.2.3",
    "@headlessui/react": "^1.7.7",
    "@heroicons/react": "^2.0.13",
    "@remix-run/cloudflare": "^1.9.0",
    "@remix-run/cloudflare-pages": "^1.9.0",
    "@remix-run/react": "^1.9.0",
    "@tailwindcss/forms": "^0.5.3",
    "browser-image-compression": "^2.0.0",
    "clsx": "^1.2.1",
    "cross-env": "^7.0.3",
    "daisyui": "^2.46.0",
    "egoroof-blowfish": "^2.2.2",
    "gsap": "^3.11.4",
    "i18next": "^22.4.6",
    "i18next-browser-languagedetector": "^7.0.1",
    "i18next-fs-backend": "^2.1.1",
    "i18next-http-backend": "^2.1.1",
    "isbot": "^3.6.5",
    "next-auth": "^4.18.7",
    "node-persist": "^3.1.0",
    "patch-package": "^6.5.0",
    "query-string": "^8.1.0",
    "react": "^18.2.0",
    "react-countdown": "^2.3.5",
    "react-daisyui": "^2.5.0",
    "react-datepicker": "^4.8.0",
    "react-div-100vh": "^0.7.0",
    "react-dom": "^18.2.0",
    "remix-i18next": "^5.0.0",
    "remix-utils": "^5.1.0",
    "ua-parser-js": "^1.0.32",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.18.0",
    "@remix-run/dev": "^1.9.0",
    "@remix-run/eslint-config": "^1.9.0",
    "@types/node-persist": "^3.1.3",
    "@types/react": "^18.0.25",
    "@types/react-datepicker": "^4.8.0",
    "@types/react-dom": "^18.0.8",
    "@types/uuid": "^9.0.0",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "eslint": "^8.27.0",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.20",
    "prettier": "^2.8.1",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "remix-esbuild-override": "^3.0.4",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.8.4",
    "wrangler": "^2.2.1"
  },
  "engines": {
    "node": ">=16.13"
  }
}
