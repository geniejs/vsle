{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:uno build:*",
    "build:remix": "cross-env NODE_ENV=production remix build",
    "build:uno": "npm run generate:uno -- --minify",
    "dev": "npm-run-all -p dev:*",
    "dev:remix": "cross-env NODE_ENV=development remix watch",
    "dev:uno": "npm run generate:uno -- --watch",
    "generate:uno": "unocss ./app/global.css ./app/**/*.{js,ts,jsx,tsx} --out-file ./app/uno.css",
    "start": "cross-env NODE_ENV=production npm run wrangler",
    "typecheck": "tsc -b",
    "dev:wrangler": "cross-env NODE_ENV=development npm run wrangler",
    "wrangler": "wrangler pages dev ./public",
    "build:worker": "esbuild ./app/entry.worker.ts --outfile=./public/entry.worker.js --minify --bundle --format=esm --define:process.env.NODE_ENV='\"production\"'",
    "dev:worker": "esbuild ./app/entry.worker.ts --outfile=./public/entry.worker.js --bundle --format=esm --define:process.env.NODE_ENV='\"development\"' --watch",
    "tail:wrangler": "wrangler pages deployment tail --project-name vsle --environment preview",
    "generate:prisma": "npx prisma generate --data-proxy",
    "postinstall": "remix-esbuild-override && patch-package"
  },
  "dependencies": {
    "@auth/core": "^0.2.5",
    "@headlessui/react": "^1.7.7",
    "@heroicons/react": "^2.0.13",
    "@iconify-json/fe": "^1.1.5",
    "@next-auth/prisma-adapter": "^1.0.5",
    "@novu/node": "^0.10.2",
    "@prisma/client": "^4.9.0",
    "@radix-ui/react-alert-dialog": "^1.0.2",
    "@remix-run/cloudflare": "^1.11.0",
    "@remix-run/cloudflare-pages": "^1.11.0",
    "@remix-run/react": "^1.11.0",
    "browser-image-compression": "^2.0.0",
    "clsx": "^1.2.1",
    "cross-env": "^7.0.3",
    "egoroof-blowfish": "^2.2.2",
    "gsap": "^3.11.4",
    "i18next": "^22.4.9",
    "i18next-browser-languagedetector": "^7.0.1",
    "i18next-fs-backend": "^2.1.1",
    "i18next-http-backend": "^2.1.1",
    "isbot": "^3.6.5",
    "next-auth": "^4.18.8",
    "node-persist": "^3.1.0",
    "onu-ui": "^1.0.13-beta.0",
    "patch-package": "^6.5.1",
    "query-string": "^8.1.0",
    "react": "^18.2.0",
    "react-countdown": "^2.3.5",
    "react-datepicker": "^4.8.0",
    "react-div-100vh": "^0.7.0",
    "react-dom": "^18.2.0",
    "remix-i18next": "^5.0.0",
    "remix-utils": "^6.0.0",
    "ua-parser-js": "^1.0.32",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.18.0",
    "@iconify-json/material-symbols": "^1.1.26",
    "@julr/unocss-preset-forms": "^0.0.2",
    "@remix-run/dev": "^1.11.0",
    "@remix-run/eslint-config": "^1.11.0",
    "@types/node-persist": "^3.1.3",
    "@types/react": "^18.0.27",
    "@types/react-datepicker": "^4.8.0",
    "@types/react-dom": "^18.0.10",
    "@types/uuid": "^9.0.0",
    "@unocss/preset-attributify": "^0.48.4",
    "@unocss/preset-icons": "^0.48.4",
    "@unocss/preset-web-fonts": "^0.48.4",
    "@unocss/preset-wind": "^0.48.4",
    "@unocss/reset": "^0.48.4",
    "@unocss/transformer-directives": "^0.48.4",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "prettier": "2.8.3",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "prisma": "^4.9.0",
    "remix-esbuild-override": "^3.0.4",
    "typescript": "^4.9.4",
    "unocss": "^0.48.4",
    "unocss-preset-theme": "^0.3.1",
    "wrangler": "^2.8.0"
  },
  "engines": {
    "node": ">=16.13"
  }
}
