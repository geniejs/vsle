{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:uno build:*",
    "build:remix": "cross-env NODE_ENV=production remix build",
    "build:uno": "npm run generate:uno -- --minify",
    "dev": "npm-run-all -p dev:*",
    "dev:remix": "cross-env NODE_ENV=development remix watch",
    "nextd:remix": "cross-env NODE_ENV=development remix dev  --port 3333 --app-server-port 8788",
    "dev:uno": "npm run generate:uno -- --watch",
    "generate:uno": "unocss ./app/**/*.{js,ts,jsx,tsx} --out-file ./app/uno.css",
    "start": "cross-env NODE_ENV=production npm run wrangler",
    "typecheck": "tsc -b",
    "dev:wrangler": "cross-env NODE_ENV=development npm run wrangler",
    "wrangler": "wrangler pages dev ./public --local --persist",
    "build:worker": "esbuild ./app/entry.worker.ts --outfile=./public/entry.worker.js --minify --bundle --format=esm --define:process.env.NODE_ENV='\"production\"'",
    "dev:worker": "esbuild ./app/entry.worker.ts --outfile=./public/entry.worker.js --bundle --format=esm --define:process.env.NODE_ENV='\"development\"' --watch",
    "tail:wrangler": "wrangler pages deployment tail --project-name vsle --environment preview",
    "postinstall": "remix-esbuild-override && patch-package"
  },
  "dependencies": {
    "@auth/core": "^0.4.0",
    "@headlessui/react": "^1.7.10",
    "@heroicons/react": "^2.0.15",
    "@iconify-json/fe": "^1.1.5",
    "@novu/node": "^0.11.0",
    "@paralleldrive/cuid2": "^2.0.1",
    "@picocss/pico": "^1.5.7",
    "@radix-ui/react-alert-dialog": "^1.0.2",
    "@remix-run/cloudflare": "^1.12.0",
    "@remix-run/cloudflare-pages": "^1.12.0",
    "@remix-run/css-bundle": "^1.12.0",
    "@remix-run/react": "^1.12.0",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "@vespaiach/axios-fetch-adapter": "^0.3.1",
    "clsx": "^1.2.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "egoroof-blowfish": "^2.2.2",
    "isbot": "^3.6.5",
    "next-auth": "^4.19.2",
    "node-persist": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "query-string": "^8.1.0",
    "react": "^18.2.0",
    "react-countdown": "^2.3.5",
    "react-datepicker": "^4.10.0",
    "react-div-100vh": "^0.7.0",
    "react-dom": "^18.2.0",
    "remix-i18next": "^5.0.0",
    "remix-utils": "^6.0.0",
    "ua-parser-js": "^1.0.33",
    "web-push": "^3.5.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.19.0",
    "@iconify-json/material-symbols": "^1.1.28",
    "@julr/unocss-preset-forms": "^0.0.3",
    "@remix-run/dev": "^1.12.0",
    "@remix-run/eslint-config": "^1.12.0",
    "@types/node-persist": "^3.1.3",
    "@types/react": "^18.0.27",
    "@types/react-datepicker": "^4.8.0",
    "@types/react-dom": "^18.0.10",
    "@types/uuid": "^9.0.0",
    "@unocss/preset-attributify": "^0.49.4",
    "@unocss/preset-icons": "^0.49.4",
    "@unocss/preset-web-fonts": "^0.49.4",
    "@unocss/preset-wind": "^0.49.4",
